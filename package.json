    "@remix-run/react": "^2.8.0",
    "clsx": "^2.1.0",
    "dayjs": "^1.11.10",
    "informed": "^4.56.3",
    "isbot": "^4.1.0",
    "netlify-cli": "^17.19.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
